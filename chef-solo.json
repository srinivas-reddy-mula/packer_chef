{
  "variables": {
    "myaccess": "",
    "mysecret": ""
  },
  "builders": [
    {
      "type": "amazon-ebs",
      "access_key": "{{user `myaccess`}}",
      "secret_key": "{{user `mysecret`}}",
      "region": "us-west-2",
      "source_ami": "ami-08692d171e3cf02d6",
      "instance_type": "t2.micro",
      "ssh_username": "ubuntu",
      "ami_name": "packer {{timestamp}}"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "inline": 
      ["sudo apt-get update","pwd"]
    },
    {
      "type": "chef-solo",
      "version": "14.11.21",
      "cookbook_paths": ["D:/DevOps_Practice/chef-starter/chef-repo/cookbooks"],
      "run_list": ["recipe[apache_cookbook::default]"]
    }
  ]
}